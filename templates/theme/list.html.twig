{% extends 'base.html.twig' %}

{% block body %}
	<h2 class="mb-5">
		{{ 'Liste'|trans }}
	</h2>

	<section class="dark">
		{% for row in tags|batch(8) %}
			<div class="container py-4">
				{% for tag in row %}
					<h4>{{ tag.name }}</h4>
				{% endfor %}
			</div>
		{% endfor %}
	</section>
{% endblock %}
